{
  "title": "Stateless IME switch",
  "rules": [
    {
      "description": "Left Command → Eisuu / Right Command → Kana",
      "manipulators": [
        {
          "from": {
            "key_code": "left_command"
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_eisuu"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "right_command"
          },
          "to": [
            {
              "key_code": "right_command"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "japanese_kana"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
